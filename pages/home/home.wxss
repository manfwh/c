.admin {
  position: fixed;
  bottom: 100rpx;
  right: 0rpx;
  width: 150rpx;
  height: 150rpx;
  text-align: center;
  line-height: 150rpx;
  background: rgba(0, 0, 0, .1);
  z-index: 20;
}


.bg-pic {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 390rpx;
}
.header-wrap {
  margin-top: 46rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 46rpx;
}
.userinfo {
}
.userinfo .username {
  align-items: flex-end;
  color: #fff;
}
.userinfo .username .text-hidden {
  max-width: 434rpx;  
  font-size: 70rpx;
  font-weight: bold;    
}
.userinfo .tag {
  width: 46rpx;
  height: 28rpx;
  border-radius: 18rpx;
  background: linear-gradient(to right, #37e7c4, #3fe083);
  font-size: 18rpx;
  text-align: center;
  line-height: 28rpx;
  margin-left: 10rpx;
  transform: translateY(-12rpx);
  padding: 4rpx;
}
.userinfo .tag.gd {
  background: #f11a5c;
}
.userinfo .usercar {
  font-size: 24rpx;
  color: #d5d9fa;
}
.userinfo .usercar text {
  margin-left: 30rpx;
}
.header-wrap .userbalance {
  text-align: right;
}
.header-wrap .userbalance-nums {
  font-size: 48rpx;
  color: #fff;
}
.header-wrap .userbalance-text {
  font-size: 22rpx;
  color: rgba(255, 255, 255, .6);
  justify-content: flex-end;
}
.header-wrap .arrow {
  width: 12rpx;
  height: 20rpx;
  margin-left: 10rpx;
}
.header-wrap .icon-moneny {
  width: 19rpx;
  height: 19rpx;
  margin-right: 10rpx;
}


/* 车辆信息  */
.block {
  border-radius: 5px;
  background: #fff;
}
.cars {
  height: 474rpx;
}
.cars .row {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-left: 32rpx;
  padding-right: 26rpx;
  background: #f1f8ff;
  height: 70rpx;
}
/* .cars .row:first-child {
  margin-top: 20rpx;
} */

.cars .row .dot {
  width: 8rpx;
  height: 8rpx;
  background: #26b569;
  border-radius: 50%;
  border: 4rpx solid #cbebe3;
  margin-left: 4rpx;
  margin-right: 30rpx;
}
.cars .row .item-key {
  flex: 1;
  font-weight: bold;
}
.cars .row .item-key, .cars .row .item-val {
  font-size: 26rpx;
  color: rgba(45, 46, 65, .9);
  font-weight: bold;
}
.cars .item-val .detail-text {
  width: 200rpx;
  text-align: right;
  margin-right: 6rpx;
}
.cars .item-val .detail {
  background: linear-gradient(to right, #3f8be0, #3771e7);
  width: 46rpx;
  height: 26rpx;
  border-radius: 17rpx;
  color: #fff;
  font-size: 18rpx;
  text-align: center;
  line-height: 26rpx;
  padding: 4rpx;
}
.cars .top-row {
  height: 84rpx;
  border-bottom: 2rpx solid #e9e9ea;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-left: 32rpx;
  padding-right: 26rpx;
}
.cars .top-row .item-key {
  color: rgba(104, 43, 242, .6);
  font-size: 28rpx;
  flex: 1;
  
}
.cars .toprow-pic {
  width: 28rpx;
  height: 33rpx;
  margin-right: 20rpx;
}
.cars .top-row .level {
  display: flex;
}
.cars .level view {
  width: 24rpx;
  height: 24rpx;
  border-radius: 50%;
  background: #75778d;
  margin-right: 14rpx;
  /* box-shadow:  */
}

.cars .level view.active {
  background: linear-gradient(to bottom, #ffd97c, #f8bc2b);  
}
.cars .car-bg-pic {
  position: relative;
  background: #f1f8ff;
  height: 121rpx;
  top: -30rpx;
  z-index: -1;
}
.cars .car-bg-pic image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 121rpx;
}
.addCarBtn {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.addCarBtn .add {
  font-size: 180rpx;
  text-align: center;
  color: #ccc;
}
.addCarBtn .addText {
  text-align: center;
  color: #ccc;
}

/* 公共  */
.notice {
  width: 700rpx;
  height: 68rpx;
  background: #fdf02e;
  border-radius: 4rpx;
  margin: 25rpx auto 0;
  font-size: 24rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  line-height: 68rpx; 
}

/* 底部  */
.footer {
  position: fixed;
  bottom: 28rpx;
  width: 700rpx;
  left: 50%;
  transform: translateX(-350rpx);
}
.footer .info-wrap {
  display: flex;
  align-items: center;
  height: 150rpx;
  border-radius: 5px;
  background: #fff;
  margin-bottom: 30rpx;  
}
.footer .info-wrap .box {
  flex: 1;
  text-align: center;
  position: relative;
}
.footer .box-top {
  font-size: 46rpx;
  color: #1956e6;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 50rpx;
}
.footer .box-top .dot {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.footer .box-top .dot1 {
  border: 4rpx solid #f9e9c8;
  background: #ebb748;
}
.footer .box-top .dot2 {
  border: 4rpx solid #c4f6e1;
  background: #3de29a;
}
.footer .box text {
  font-size: 24rpx;
  color: rgba(45, 46, 65, .4);

}

.footer .box .dots {
  display: flex;
}
.footer .dots view {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  margin-right: 6rpx;
}
.footer .dots view:nth-child(1) {
  background: #dadef6;
}
.footer .dots view:nth-child(2) {
  background: #b5bded;
}
.footer .dots view:nth-child(3) {
  background: #8f9be3;
}
.footer .dots view:nth-child(4) {
  background: #6a7ada;
}
.footer .arrow {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  background: #4559d1;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 20rpx;
}
.footer .arrow image {
  width: 12rpx;
  height: 20rpx;
}
.footer .msg image {
  width: 47rpx;
  height: 68rpx;
}
.footer .msg view {
  font-size: 18rpx;
  color: #1956e6;
}
.footer .box:first-child::after, .footer .box:last-child::after {
  content: '';
  position: absolute;
  top: 50%;
  width: 2rpx;
  height: 66rpx;
  background: #e5e5e5;
  transform: translateY(-33rpx);
}
.footer .box:first-child::after {
  right: 0;
}
.footer .box:last-child::after {
  left: 0;
}
.footer .share-btn {
  height: 120rpx;
  width: 552rpx;
  background: linear-gradient(120deg, #3b1eec, #1a33c9);
  border-radius: 5px;
  font-size: 32rpx;
  color: #fff;
}
.footer .share-btn image {
  width: 44rpx;
  height: 42rpx;
  margin-right: 20rpx;
}
.footer .me-btn {
  width: 120rpx;
  height: 120rpx;
  border: 2rpx solid #1956e6;
  border-radius: 5px;
  box-sizing: border-box;
}
.footer .me-btn image {
  width: 41rpx;
  height: 41rpx;
}


/* 新用户弹窗  */
.modal {
  background: rgba(35, 36, 42, .9);
  opacity: 0;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: flex-start;
}
.modal .newUsrWrap {
  position: absolute;
  width: 597;
  height: 764rpx;
  left: 50%;
  top: 50%;
  margin-left: -274rpx;
  margin-top: -382rpx;
  transform: scale(0);
}

.modal .newUsrWrap image {
  width: 548rpx;
  height: 572rpx;
}
.modal .newUsrWrap .btn {
  width: 400rpx;
  height: 100rpx;
  border: 6rpx solid #ffbd1f;
  font-size: 40rpx;
  color: #fff;
  background: linear-gradient(to right, #f74f22, #fb2a29);
  text-align: center;
  line-height: 100rpx;
  border-radius: 66rpx;
  margin: 85rpx auto 0;
}
.modal .newUsrWrap .close {
  width: 60rpx;
  height: 60rpx;
  border: 4rpx solid #ffd800;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
  position: absolute;
  top: 0;
  right: 0;
  border-radius: 50%;
}